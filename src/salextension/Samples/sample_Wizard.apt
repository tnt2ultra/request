.head 6 +  Case 0
.head 7 +  If rbJump1
.head 8 -  Return SetStep(1,FALSE )
.head 7 +  If rbJump3
.head 8 -  Return SetStep(3,FALSE )
.head 7 -  Break
.head 6 +  Case 1
.head 7 -  ! Set to TRUE, so it ends in step 2
.head 7 -  Return SetStep(2,TRUE )

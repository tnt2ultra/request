		Functions
			Function: SalWinFillBgkGradient
				Description: author: 	???
						date:	???
						version:	???
						
						Füllt einen Windowhintergrund mit einem Farbübergang von Oben nach Unten
						 nColorBegin: 	Farbe oben
						 nColorEnd:	Fabe unten
						
				Returns
				Parameters
					Window Handle: phWnd
					Number: nColorBegin
					Number: nColorEnd
				Static Variables
				Local variables
					Number: hDC
					Number: nLeft
					Number: nTop
					Number: nRight
					Number: nBottom
					Number: hBrush
					Number: hDCMem
					Number: nBitmap
					Number: nBitmapOld
					Number: nRed
					Number: nGreen
					Number: nBlue
					Number: nRed2
					Number: nGreen2
					Number: nBlue2
					Number: nStepRed
					Number: nStepGreen
					Number: nStepBlue
					Number: nCpt
					Number: nColorBits
					Number: nNbRgn
					Number: nStep
					String: sPaint
				Actions
					Call SalStrSetBufferLength( sPaint, 64 )
					Call BeginPaint( phWnd, sPaint )
					Set hDC=GetDC(phWnd)
					! - Récupération dimensions client
					Call GetClientRect( phWnd, nLeft, nTop, nRight, nBottom )
					! - Création DC compatible avec DC client
					Set hDCMem=CreateCompatibleDC(hDC)
					! - Création Bitmap compatible avec Bitmap client
					Set nBitmap=CreateCompatibleBitmap(hDC,nRight,nBottom)
					Set nBitmapOld = SelectObject(hDCMem,nBitmap)
					! - Calcul de bits par couleur et nombre de regions
					Set nColorBits = GetDeviceCaps( hDC, BITSPIXEL ) * GetDeviceCaps( hDC, PLANES )
					Select Case nColorBits
						Case 32
						Case 24
						Case 16
							Set nNbRgn = 256
							Break
						Case 15
							Set nNbRgn = 32
							Break
						Default
							Set nNbRgn = 64
					! - Initialisations
					Set nStep = nBottom / nNbRgn
					Call SalColorToRGB( nColorBegin, nRed, nGreen, nBlue )
					Call SalColorToRGB( nColorEnd, nRed2, nGreen2, nBlue2 )
					Set nStepRed = (nRed2 - nRed ) / (nNbRgn - 1)
					Set nStepGreen = (nGreen2 - nGreen ) / (nNbRgn - 1)
					Set nStepBlue = (nBlue2 - nBlue ) / (nNbRgn - 1)
					Set nCpt = 0
					! - Gradient
					While nCpt < nNbRgn
						Set hBrush = CreateSolidBrush( SalColorFromRGB(nRed, nGreen, nBlue) )
						Set nBottom =  nTop + nStep
						Call FillRect( hDCMem, nLeft, nTop , nRight, nBottom, hBrush )
						Set nTop = nBottom
						Call DeleteObject( hBrush )
						Set nRed = nRed + nStepRed
						Set nGreen = nGreen + nStepGreen
						Set nBlue = nBlue + nStepBlue
						Set nCpt = nCpt + 1
					Call GetClientRect( phWnd, nLeft, nTop, nRight, nBottom )
					Call BitBlt(hDC, 0, 0, nRight, nBottom, hDCMem,0,0,SRCCOPY)
					! - Empêche la fenêtre d'être repeinte
					! - Restaure la mémoire
					Call SelectObject(hDCMem,nBitmapOld)
					Call DeleteObject( nBitmap )
					Call DeleteDC( hDCMem )
					Call ReleaseDC( phWnd, hDC )
					Call EndPaint( phWnd, sPaint )
		Window Parameters
		Window Variables
		Message Actions
			On WM_PAINT
				! fill gradient
				Call SalWinFillBgkGradient(hWndForm, 0xffefaa,0x000000 )
				Call SalWinFillBgkGradient(SalTBarGetHandle (hWndForm), 0xffefaa,0x000000 )
				Call SalWinFillBgkGradient(SalStatusGetHandle (hWndForm), 0xffefaa,0x000000 )
				! redraw
				Call SalInvalidateChildren(hWndForm)
				Call SalInvalidateChildren(SalTBarGetHandle(hWndForm))
				Call SalInvalidateChildren(SalStatusGetHandle (hWndForm) )

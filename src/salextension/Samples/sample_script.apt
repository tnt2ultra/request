				Actions 
					Set lbResult = FALSE
					Set lsScript = ''
					Set lbIFLevelValue[0] = TRUE
					!
					If fpsFileName != STRING_Null And SalFileOpen(lfhScript, fpsFileName, OF_Read)
						Call SalWaitCursor( TRUE )
						Set lbResult = TRUE
						! Script Einlesen
						While SalFileGetStr(lfhScript, lsLine, 2000)
							! when comment, or nothing -> ignore
							If SalStrTrimX(SalStrLeftX(lsLine,1)) != ';' and SalStrTrimX(lsLine) != STRING_Null
								!
								! security Check
								If SalStrScan(lsLine,'SqlPassword') > -1
									Set lsLine =  VisStrSubstitute(lsLine, 'SqlPassword', '')
									If not pbQuietMode
										Call SalMessageBox("Script '" || fpsFileName || "' Security Alert !", "Warning", MB_Ok | MB_IconExclamation)
								!
								! ok Execute line
								Else 
									! Only execute this level, when if it is  allowed ( needed for sub-IF´s)
									If SalStrLeftX(SalStrLowerX(lsLine),5) = 'endif'
										If lnIFLevel > 0
											Set lnIFLevel = lnIFLevel - 1
									Else If lbIFLevelValue[ lnIFLevel] = TRUE
										!
										! 'If' ??
										If SalStrLeftX(SalStrLowerX(lsLine),3) = 'if '
											Set nType =  SalCompileAndEvaluate(lsLine ,nError, nErrorPos, nReturn, strReturn, dtReturn, hWndReturn, pbQuietMode, SalContextCurrent() )
											! when 'if' was true
											Set lnIFLevel = lnIFLevel + 1
											If nReturn = 1
												Set lbIFLevelValue[ lnIFLevel] = TRUE
											Else 
												Set lbIFLevelValue[ lnIFLevel] = FALSE
										! 'While' is not implemented yet (if needed, will be done here
										!
										! 'normal' expression
										Else 
											Call SalCompileAndEvaluate(lsLine ,nError, nErrorPos, nReturn, strReturn, dtReturn, hWndReturn, pbQuietMode, SalContextCurrent() )
						Call SalWaitCursor( FALSE )
						Call SalFileClose(lfhScript)
					Else 
						If not pbQuietMode
							Call SalMessageBox("Script '" || fpsFileName ||  "' not found !", "Error", MB_Ok | MB_IconExclamation)
					Return lbResult

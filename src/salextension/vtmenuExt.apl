Set wnPic[3] = lhPic3

Select Case VisMenuPopupPicture( wsTest, wnPic,TPM_CursorX,TPM_CursorY)
        Case 0
  	Break
        Case 1
  	Break
        Case 2
  	Break
        Case 3
  	Break
